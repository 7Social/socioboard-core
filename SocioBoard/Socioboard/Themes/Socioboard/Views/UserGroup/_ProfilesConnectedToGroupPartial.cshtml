         <div class="delet_icon"  onclick="removeprofilefromgroup('@objTwitterAccount.TwitterUserId');"></div>
        <img src="@objTwitterAccount.ProfileImageUrl" class="img-responsive img-rounded groupprofiles" title="@objTwitterAccount.TwitterScreenName">
        <img class="img-responsive img-circle groupsocialIconPoss" src="/Themes/@path/Contents/img/twticon.png">
    </div>
}
    }
    if (item.Key.ProfileType == "linkedin")
    {
        LinkedInAccount objLinkedInAccount = (LinkedInAccount)item.Value;
        if (objLinkedInAccount != null)
