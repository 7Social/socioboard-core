        <img class="img-responsive img-circle groupsocialIconPoss" src="/Themes/Socioboard/Contents/img/twticon.png">
    </div>
        }
    }
    if (item.Key.ProfileType == "linkedin")
    {
        LinkedInAccount objLinkedInAccount = (LinkedInAccount)item.Value;
        if (objLinkedInAccount != null)
        {

            string profileimage = string.Empty;
