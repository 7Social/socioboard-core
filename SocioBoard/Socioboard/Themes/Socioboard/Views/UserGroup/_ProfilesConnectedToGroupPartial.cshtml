@using Domain.Socioboard.Domain;
@{
    string path = @System.Configuration.ConfigurationManager.AppSettings["domain"];
    User objUser = (User)Session["User"];
    
}



<h4>Profile Linked To<span id="groupname"> DotNet</span></h4>
@*<span class="pull-right"><a role="button" class="btn btn-warning btn-sm">Connect a profile</a></span>*@

@foreach (var item in (Dictionary<GroupProfile, object>)Model)
{
    if (item.Key.ProfileType == "facebook")
    {
        FacebookAccount objFacebookAccount = (FacebookAccount)item.Value;
        if(objFacebookAccount!=null)
        {
        string profileimage = string.Empty;
    <div id="@objFacebookAccount.FbUserId" class="col-sm-1 groupuserpictiny" >
       @*<div data-target="#closeModal" data-toggle="modal" onclick="confirmDel('415298848564891','page','fb');" class="add_icon"></div>*@
         <div class="delet_icon" onclick="removeprofilefromgroup('@objFacebookAccount.FbUserId');"></div>
        <img src="http://graph.facebook.com/@objFacebookAccount.FbUserId/picture?type=small" class="img-responsive img-rounded groupprofiles" title="@objFacebookAccount.FbUserName">
        <img class="img-responsive img-circle groupsocialIconPoss" src="/Themes/@path/Contents/img/fb_icon.png">
    </div>
}
    }
    if (item.Key.ProfileType == "twitter")
    {
        TwitterAccount objTwitterAccount = (TwitterAccount)item.Value;
        if(objTwitterAccount!=null)
        {
        string profileimage = string.Empty;
