    </div>
        }
    }
    if (item.Key.ProfileType == "youtube")
    {
        YoutubeAccount objYoutubeAccount = (YoutubeAccount)item.Value;
        if (objYoutubeAccount != null)
        {
            string profileimage = string.Empty;
    <div id="@objYoutubeAccount.Ytuserid" class="col-sm-1 groupuserpictiny"  >
        <div class="delet_icon" onclick="removeprofilefromgroup('@objYoutubeAccount.Ytuserid');"></div>
