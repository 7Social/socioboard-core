            minm = '0' + minm;
        }
        else {
            minm = curdate.getMinutes();
        }
        var sec = curdate.getSeconds();
        if (sec < 10) {
            sec = '0' + sec;
        }
        else {
            sec = curdate.getSeconds();
        }
        var allprofiles = '';
        var now = curdate.getFullYear() + "/" + mm + "/" + dd + " " + hh + ":" + minm + ":" + sec;
        var fd = new FormData();
        var filesinput = document.getElementById('schedulebulkupload').files[0];

        //filename = document.getElementById('schedulebulkupload');


        for (var i = 0; i < arrbinduser.length; i++) {
            allprofiles += arrbinduser[i] + "<:>";
        }
        allprofiles = allprofiles.substr(0, allprofiles.length - 3);
        fd.append("filesinput", filesinput);
        $.ajax({
            type: 'POST',
            url: '../Publishing/InputCSVForBulkImport?profiles=' + allprofiles + '&clienttime=' + now,
            data: fd,
            processData: false,
            contentType: false,
            success: function (msg) {
                debugger;
                alert(msg);
                $('.bindprof').hide(1000, function () {
                    $('#bindcomposescheduledprofiles').html('');
                    $('#bindcomposescheduledprofiles').val('');
                    $('#schedulebulkupload').val('');
                });

            },
            async: false
        });




    }


</script>
